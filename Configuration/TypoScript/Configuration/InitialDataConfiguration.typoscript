                        as = primaryNavigation
                    }
                }
            }
            i18n = JSON
            i18n {
                dataProcessing {
                    10 = TYPO3\CMS\Frontend\DataProcessing\LanguageMenuProcessor
                    10 {
                        languages = auto
                        as = languages
                    }
                }
            }
        }
    }
}
